<link href="/css/userauth/register-success.css" rel="stylesheet" />
<meta content="/userauth/register-success" name="ac:route">
<div id="browser" is="dmx-browser">
</div>
<dmx-cookie-manager id="cookies">
</dmx-cookie-manager>
<dmx-notifications id="notify" timeout="500">
</dmx-notifications>
<dmx-serverconnect id="get_info" url="/api/userauth/account/info">
</dmx-serverconnect>
<dmx-value dmx-bind:value="get_info.data.roles.values('role').contains('admin')" id="is_admin">
</dmx-value>
<dmx-value dmx-bind:value="(get_info.data.identity?true:false)" id="is_user">
</dmx-value>
<div class="d-flex" fid="42:1911" id="register-success">
    <span class="Black_color Headline_5" fid="42:1912" fname="An email has been sent to you containing a validation link. Please check you junk folder if you are unable to find it." id="_42_1912">
        An email has been sent to you containing a validation link.
        <br>
        <br>
        Please check you junk folder if you are unable to find it.
    </span>
</div>